<Window x:Class="SiteHealthMonitor.Views.ActualVersionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SiteHealthMonitor.Views"
        mc:Ignorable="d"
        Title="У вас установлена актуальная версия" SizeToContent="Height" Width="350" ResizeMode="NoResize">
    <StackPanel Margin="30">
        <TextBlock TextAlignment="Center">У вас установлена актуальная версия</TextBlock>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,15,0,0">
            <Button Content="Ок" Margin="0,0,10,0" Click="CancelButton"/>
        </StackPanel>
    </StackPanel>
</Window>
